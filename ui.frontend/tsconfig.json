{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "strict": true,
        "moduleResolution":"node",
        "baseUrl": "../ui.frontend",
        "removeComments": true,
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "preserveConstEnums": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true
    },
    "include": [ "./src/**/*.ts" ]
}
